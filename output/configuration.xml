<?xml version="1.0" encoding="UTF-8"?>

<PhysiCell_settings version="devel-version">
	<domain>
		<x_min>0</x_min>
		<x_max>3000</x_max>
		<y_min>0</y_min>
		<y_max>3000</y_max>
		<z_min>-10</z_min>
		<z_max>10</z_max>
		<dx>50</dx>
		<dy>50</dy>
		<dz>50</dz>
		<use_2D>true</use_2D>
	</domain>
	
	<overall>
		<max_time units="min">250</max_time> 
		<time_units>min</time_units>
		<space_units>micron</space_units>
	</overall>
	
	<parallel>
		<omp_num_threads>1</omp_num_threads>
	</parallel> 
	
	<save>
		<folder>output</folder> <!-- use . for root --> 

		<full_data>
			<interval units="min">15</interval>
			<enable>true</enable>
		</full_data>
		
		<SVG>
			<interval units="min">15</interval>
			<enable>true</enable>
		</SVG>
	</save>

	<microenvironment_setup>
		<variable name="death_signal" units="particles/micron^3" ID="0">
			<physical_parameter_set>
				<diffusion_coefficient units="micron^2/min">10.0</diffusion_coefficient>
				<decay_rate units="1/min">0</decay_rate>  
			</physical_parameter_set>
			<initial_condition units="particles/micron^3">0</initial_condition>
			<Dirichlet_boundary_condition units="particles/micron^3" enabled="false">0</Dirichlet_boundary_condition>
		</variable>
	
		<options>
			<calculate_gradients>false</calculate_gradients>
			<track_internalized_substrates_in_each_agent>true</track_internalized_substrates_in_each_agent>
		</options>
	</microenvironment_setup>	
	
	<user_parameters>
		<random_seed type="int" units="dimensionless" hidden="true">0</random_seed> 
		<csv_file type="string" description="cell info">exp_a.csv</csv_file> 
		<csv_file2 type="string" description="neighbors info">exp_b.csv</csv_file2>
		<cell_radius type="double">20.0</cell_radius> 
		<death_threshold type="double">1</death_threshold> 
		<initial_internalized_substrate type="double">0.8</initial_internalized_substrate> 
		<min_death_count type="double">10</min_death_count> 
		<signal_internalization_rate type="double" units="1/min" description="internalization rate for death particles">10</signal_internalization_rate> 
		<signal_diffusion_coefficient type="double" units="micron^2/min" description="diffusion coefficient for death signal particles">500</signal_diffusion_coefficient>
	</user_parameters>
	
	
</PhysiCell_settings>
